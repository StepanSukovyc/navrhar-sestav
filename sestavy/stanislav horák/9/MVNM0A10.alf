<![CDATA[
def obec(self,radek,pobalka,pmezera,obaladr):  
  if pobalka=='1':
    adresa=obaladr
  else:
    adresa=self.value
    
  if radek==1:
    if pmezera=='0':
      self.value=adresa
    else:
      self.value=''  
